rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /thumbnails/{allPaths=**} {
      allow read;
      allow write: if request.auth != null;
    }
    match /files/{allPaths=**} {
      allow write: if request.auth != null;
      allow read: if false;
    }
    match /{allPaths=**} {
      allow read: if false;
      allow write: if false;
    }
  }
}
